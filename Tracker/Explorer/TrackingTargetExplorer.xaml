<Window x:Class="Tracker.Explorer.TrackingTargetExplorer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Tracker.Explorer"
        mc:Ignorable="d"
        Title="Tracking Target Explorer"
        Height="300" Width="400" WindowStartupLocation="CenterScreen">
    <Window.InputBindings>
        <KeyBinding Modifiers="Ctrl" Key="R" Command="{Binding CtrlR}" />
    </Window.InputBindings>
    <Grid>
        <local:TrackingTargetPreview x:Name="preview" x:FieldModifier="internal" />
    </Grid>
</Window>
